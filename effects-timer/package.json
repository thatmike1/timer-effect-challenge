{
  "alfons": true,
  "author": "michal.psencik",
  "dependencies": {
    "@alfons-app/pdk": "^0.11.1",
    "@fluentui/react-icons": "^2.0.271"
  },
  "devDependencies": {
    "@alfons-app/eslint-config": "^0.11.1",
    "@alfons-app/sandbox": "^0.11.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^12.7.2",
    "@types/jest": "^29.5.13",
    "babel-jest": "^29.7.0",
    "babel-preset-expo": "^12.0.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-expo": "^52.0.3",
    "react": "^18.2.0",
    "typescript": "^5.4.4"
  },
  "eslintConfig": {
    "extends": [
      "@alfons-app"
    ],
    "root": true
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "passWithNoTests": true,
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/*.test.tsx"
    ],
    "transform": {
      "^.+\\.(ts|tsx|js|jsx)$": [
        "babel-jest",
        {
          "presets": [
            "babel-preset-expo"
          ]
        }
      ]
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@alfons-app|jsonpath-plus)/)"
    ]
  },
  "license": "",
  "name": "@michal-psencik/effects-timer",
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://registry.npmjs.org/",
    "scope": "michal-psencik"
  },
  "scripts": {
    "lint": "eslint . --fix",
    "sandbox": "sandbox",
    "test": "jest --silent",
    "test:watch": "jest --watchAll",
    "typecheck": "tsc"
  },
  "version": "0.0.0"
}
